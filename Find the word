#include <iostream>
#include <string>

using namespace std;

int main() {
    string s = "hello world! uwu";
    string result;

    size_t firstIndex = s.find(' ');
    
    size_t secondIndex = s.find(' ', firstIndex + 1);
    
    string secondWord = s.substr(firstIndex + 1, secondIndex - firstIndex - 1);
    
    cout << secondWord;
}
